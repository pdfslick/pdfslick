{
  "name": "pdfslick",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "config/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "turbo run build lint && changeset version",
    "publish-packages": "changeset publish",
    "update-deps": "ncu -ws -i"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint-config-custom": "*",
    "npm-check-updates": "^16.14.5",
    "prettier": "latest",
    "turbo": "^1.10.15"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "rollup": "^4.0.2"
  },
  "packageManager": "npm@9.8.0"
}
